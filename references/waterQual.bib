@article{kankaalaVariableProductionDifferent2013,
  title = {Variable {{Production}} by {{Different Pelagic Energy Mobilizers}} in {{Boreal Lakes}}},
  volume = {16},
  issn = {14329840},
  doi = {10.1007/s10021-013-9674-z},
  abstract = {We studied production by three key pelagic energy mobilizer communities, phytoplankton (PP), heterotrophic bacteria (HB), and methanotrophic bacteria (MOB), in five boreal lakes of varying size and concentration of dissolved organic carbon (DOC). Production by PP was responsible for most ($\backslash$textgreater55\%) of the total pelagic energy mobilization in all five lakes. Production by HB and PP estimated for the whole water column during the ice-free period were positively correlated, but with the exception of the clearest and most eutrophic lake PP apparently could not support the total carbon demand of bacteria. However, the DOC concentration did not explain the variability of heterotrophic bacterial production (HBP) within or between the lakes. Thus, our results provide circumstantial evidence for the “priming effect” whereby labile organic matter from autochthonous production enhances decomposition of allochthonous DOC. However, HBP was only 10–23\% of the total pelagic energy mobilization in the lakes, suggesting that only a minor fraction of allochthonous DOC became available for higher trophic levels. High MOB activity was detected in the water columns of the stratified lakes when the molar ratio of CH4:O2 varied between 0.5 and 12. In the small stratified lakes (area $\backslash$textless 0.01 km2), MOB production contributed 13–52\% of the total pelagic energy mobilization, being greatest during the autumn mixing period. Our results indicate that in small stratified lakes (area $\backslash$textless 0.01 km2) bacteria, especially MOB, are potentially quantitatively important supplementary food resources for zooplankton. However, in larger lakes primary producers are the most important ($\backslash$textgreater70\%) potential food source for zooplankton.},
  number = {6},
  journaltitle = {Ecosystems},
  date = {2013},
  pages = {1152--1164},
  keywords = {pelagic food web,Dissolved organic carbon,bacterial production,primary production,methane oxidation,priming effect,bacterial pro-,duction},
  author = {Kankaala, Paula and Lopez Bellido, Jessica and Ojala, Anne and Tulonen, Tiina and Jones, Roger I.},
  file = {C:\\Users\\nicol\\Zotero\\storage\\ZQ7233MJ\\Kankaala et al. - 2013 - Variable Production by Different Pelagic Energy Mobilizers in Boreal Lakes.pdf}
}

