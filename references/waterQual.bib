@incollection{robartsBacteriaBacterioplankton2009,
  location = {{Oxford}},
  title = {Bacteria, {{Bacterioplankton}}},
  isbn = {978-0-12-370626-3},
  url = {http://www.sciencedirect.com/science/article/pii/B9780123706263001241},
  abstract = {Bacteria have been known to inhabit natural waters since the seventeenth century, but it was not until the mid to late twentieth century that our knowledge of their quantitative role in nutrient and energy cycles became apparent. This was due to the development of new techniques in microscopy and the application of radiolabeled compounds. While our knowledge has undergone a quantum leap, there are still many controversies and unknowns remaining in our understanding of bacterioplankton in aquatic systems. We know that they frequently exist in numbers of millions per milliliter, they are small (median cell volumes usually between 0.013 and 0.200 μm3), and most are metabolically inactive (around 80\% on average), but those that are active are key drivers of biogeochemical cycles. However, robust explanations of why cross-system changes in bacterioplankton biomass and production are not proportional to similar phytoplankton changes and the prevalence of net heterotrophic systems in inland waters are two major areas requiring further research. Bacterioplankton are a key link between abiotic and biotic components of aquatic ecosystems, but our understanding of how they respond to climate-induced impacts and human-made chemicals is still limited.},
  publisher = {{Academic Press}},
  date = {2009},
  pages = {193--200},
