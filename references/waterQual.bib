@article{stoddardContinentalScaleIncreaseLake2016,
  title = {Continental-{{Scale Increase}} in {{Lake}} and {{Stream Phosphorus}}: {{Are Oligotrophic Systems Disappearing}} in the {{United States}}?},
  volume = {50},
