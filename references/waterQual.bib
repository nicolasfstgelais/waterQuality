  file = {C:\\Users\\nicol\\Zotero\\storage\\68SFWF9V\\Prepas et al. - 2001 - Landscape variables influencing nutrients and phytoplankton communities in Boreal Plain lakes of northern Albe(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\7B63ZBET\\Prepas et al. - 2001 - Landscape variables influencing nutrients and phytoplankton communities in Boreal Plain lakes of northern Alberta.pdf;C:\\Users\\nicol\\Zotero\\storage\\AJGIK9FZ\\Prepas et al. - 2001 - Landscape variables influencing nutrients and phytoplankton communities in Boreal Plain lakes of northern Alberta.pdf},
