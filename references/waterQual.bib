  author = {Longmuir, Allyson and Shurin, Jonathan B and Clasen, Jessica L},
  file = {C:\\Users\\nicol\\Zotero\\storage\\B3TS2JGT\\Longmuir, Shurin, Clasen - 2007 - Independent gradients of producer, consumer, and microbial diversity in lake plankton.pdf},
