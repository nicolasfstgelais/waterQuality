  keywords = {Models,Biological Evolution,Species Specificity,Genetic,Phylogeny,Biodiversity,Biota,Biomass,ontario,Computational Biology,Angiosperms,Angiosperms: growth & development,Carbon Sequestration,Carbon Sequestration: physiology,Likelihood Functions,Linear Models},
  author = {Cadotte, Marc W},
  file = {C:\\Users\\nicol\\Zotero\\storage\\JMXV6J5R\\Cadotte - 2013 - Experimental evidence that evolutionarily diverse assemblages result in higher productivity.pdf},
  eprinttype = {pmid},
