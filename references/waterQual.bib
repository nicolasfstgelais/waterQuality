  file = {C:\\Users\\nicol\\Zotero\\storage\\DMIJJQSB\\Swenson, Anglada-Cordero, Barone - 2011 - Deterministic tropical tree community turnover evidence from patterns of functional beta diver.pdf},
  eprinttype = {pmid},
