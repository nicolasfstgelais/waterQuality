@book{fontanetoBiogeographyMicroscopicOrganisms2011,
  title = {Biogeography of Microscopic Organisms: Is Everything Small Everywhere?},
  isbn = {1-139-49658-1},
  abstract = {Bringing together the viewpoints of leading experts in taxonomy, ecology and biogeography of different taxa, this book synthesises discussion surrounding the so-called 'everything is everywhere' hypothesis. It addresses the processes that generate spatial patterns of diversity and biogeography in organisms that can potentially be cosmopolitan. The contributors discuss questions such as: are microorganisms (e.g. prokaryotes, protists, algae, yeast and microscopic fungi, plants and animals) really cosmopolitan in their distribution? What are the biological properties that allow such potential distribution? Are there processes that would limit their distribution? Are microorganisms intrinsically different from macroscopic ones? What can microorganisms tell us about the generalities of biogeography? Can they be used for experimental biogeography? Written for graduate students and academic researchers, the book promotes a more complete understanding of the spatial patterns and the general processes in biogeography.},
  pagetotal = {284–306},
  publisher = {{Cambridge University Press}},
  date = {2011},
  keywords = {biogeography of microscopic organisms,blank,bringing together the viewpoints,ecology and,is everything small everywhere,of leading experts in,s page intentionally left,taxonomy},
  author = {Fontaneto, Diego},
