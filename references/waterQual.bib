@article{richmanEffectFoodQuality1983,
  title = {The {{Effect}} of {{Food Quality}} on {{Feeding}} and {{Respiration}} by {{Daphnia}} and {{Diaptomus}}},
  volume = {28},
  abstract = {Filtering, ingestion, and respiration rates for Daphnia pulex and Diaptomus siciloides were measured in southern Green Bay water mixed with algal cultures to produce varying proportions of blue-green algae. These measurements along with observations of Daphnia mouthpart responses to these foods were used to assess the relative competitive potential of Daphnia and calanoid copepods in lakes of varying trophy. Ratios of filtering and ingestion rate to respiration rate were used as a measure of energy input per unit of energy expended. passive filtering and subsequent rejection of inedble algae by Daphnia result in higher ratios than those shown by Diaptomus in high quality foods, suggesting a competitive advantage in oligotrophic or mesotrophic lakes. Diaptomus selective feeding, on the other hand, results in higher ratios in the low quality food characteristic of eutrophic conditions. These results and data from the literature are used to propose a two-dimensional niche model to demonstrate the interaction between food quality and abundance on the competitive interaction between calanoid copepods and Daphnia-like cladocerans.},
  number = {5},
  journaltitle = {Limnology and Oceanography},
  date = {1983},
  pages = {948--956},
  author = {Richman, Sumner and Dodson, Stanley I},
  file = {C:\\Users\\nicol\\Zotero\\storage\\EBBIQADI\\Richman, Dodson - 1983 - The effect of food quality on feeding and respiration by Daphnia and Diaptomus.pdf;C:\\Users\\nicol\\Zotero\\storage\\EZ3D7KZ4\\Richman, Dodson - 1983 - The effect of food quality on feeding and respiration by Daphnia and Diaptomus(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\K5TER539\\Richman, Dodson - 1983 - The effect of food quality on feeding and respiration by Daphnia and Diaptomus.pdf;C:\\Users\\nicol\\Zotero\\storage\\N2E49SX8\\Richman, Dodson - 1983 - The effect of food quality on feeding and respiration by Daphnia and Diaptomus(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\SIBCH883\\Richman, Dodson - 1983 - The effect of food quality on feeding and respiration by Daphnia and Diaptomus.pdf}
