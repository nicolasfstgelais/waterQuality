@article{paceDoesTerrestrialOrganic2007,
  eprinttype = {jstor},
  eprint = {4502367},
  title = {Does {{Terrestrial Organic Carbon Subsidize}} the {{Planktonic Food Web}} in a {{Clear}}-{{Water Lake}}?},
  volume = {52},
  abstract = {Allochthonous organic carbon can subsidize consumers in aquatic systems, but this subsidy may only be significant in relatively small systems with high organic matter loading. We tested the importance of allochthonous carbon to consumers in a relatively large ($\backslash$textlesstex-math$\backslash$textgreater\$258,000 m\^2\$$\backslash$textless/tex-math$\backslash$textgreater) clear-water lake by adding $\backslash$textlesstex-math$\backslash$textgreater\$H\^\{13\}CO\_3\$$\backslash$textless/tex-math$\backslash$textgreater daily for 56 d. Dissolved inorganic carbon (DIC) was substantially enriched in $\backslash$textlesssup$\backslash$textgreater13$\backslash$textless/sup$\backslash$textgreaterC by the addition, but it was also variable over diel cycles because of exchange with the atmosphere and photosynthesis. By measuring the δ$\backslash$textlesssup$\backslash$textgreater13$\backslash$textless/sup$\backslash$textgreaterC value of a physically separated phytoplankton concentrate as well as the δ$\backslash$textlesssup$\backslash$textgreater13$\backslash$textless/sup$\backslash$textgreaterC of phospholipid fatty acids, we were able to follow $\backslash$textlesstex-math$\backslash$textgreater\$\^\{13\}C-labeling\$$\backslash$textless/tex-math$\backslash$textgreater dynamics of specific groups of phytoplankton and bacteria. The δ$\backslash$textlesssup$\backslash$textgreater13$\backslash$textless/sup$\backslash$textgreaterC values of particulate organic carbon (POC), dissolved organic carbon (DOC), phytoplankton, bacteria, zooplankton, and the invertebrate predator, Chaoborus spp. all increased to a maximum during the addition and declined once the addition ceased. Autochthony (\% C derived from internal primary production) of carbon pools (POC, DOC) and consumers was assessed by fitting dynamic models to time series of δ$\backslash$textlesssup$\backslash$textgreater13$\backslash$textless/sup$\backslash$textgreaterC. Autochthonous carbon was the dominant source (88-100\%) for POC, gram-positive bacteria, a copepod, zooplankton biomass, and Chaoborus spp. Autochthonous carbon provided a lower fraction ($\backslash$textless70\%) of carbon to DOC, gram-negative bacteria, and cladoceran zooplankton. In comparison to smaller and more humic lakes, terrestrially derived allochthonous C was less significant to the pelagic food web in this larger, clear-water lake. Among lakes, the relative importance of autochthonous versus allochthonous carbon to planktonic consumers is positively correlated to the ratio of color (absorbance of light at 440 nm, an indicator of terrestrially derived organic carbon) to chlorophyll.},
  number = {5},
  journaltitle = {Limnology and Oceanography},
  date = {2007},
  pages = {2177--2189},
  author = {Pace, Michael L and Carpenter, Stephen R and Cole, Jonathan J and Coloso, James J and Kitchell, James F and Hodgson, James R and Middelburg, Jack J and Preston, Nicholas D and Solomon, Christopher T and Weidel, Brian C}
