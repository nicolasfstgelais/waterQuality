  journaltitle = {Oecologia},
  date = {2009-07},
  pages = {781--93},
  keywords = {Animals,Models,Biological,Species Specificity,Ecosystem,Environment,Ecology,Ecology: methods,Chiroptera,Feeding Behavior,Chiroptera: physiology,Demography,Feeding Behavior: physiology,Paraguay},
  author = {Presley, Steven J and Higgins, Christopher L and López-González, Celia and Stevens, Richard D},
  file = {C:\\Users\\nicol\\Zotero\\storage\\JCI5M4ED\\Presley et al. - 2009 - Elements of metacommunity structure of Paraguayan bats multiple gradients require analysis of multiple ordinatio.pdf},
