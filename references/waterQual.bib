  author = {Carpenter, Stephen R. and Cole, Jonathan J. and Hodgson, James R. and Kitchell, James E. and Pace, Michael L. and Bade, Darren and Cottinngham, Kathryn L. and Essington, Timothy E. and Houser, Jeffrey N. and Schindler, Daniel E.},
