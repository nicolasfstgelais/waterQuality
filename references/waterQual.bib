  pages = {326--335},
  keywords = {Cyanobacteria,Chlorophyll concentrations,Metal ions,Phytoplankton bioassay,Phytoplankton populations},
  author = {Oberholster, P. J. and Myburgh, J. G. and Ashton, P. J. and Botha, A. M.},
  file = {C:\\Users\\nicol\\Zotero\\storage\\HW4R7K9V\\Oberholster et al. - 2010 - Responses of phytoplankton upon exposure to a mixture of acid mine drainage and high levels of nutrient poll.pdf;C:\\Users\\nicol\\Zotero\\storage\\R3SWR655\\Oberholster et al. - 2010 - Responses of phytoplankton upon exposure to a mixture of acid mine drainage and high levels of nutrient poll.pdf},
