  keywords = {Phytoplankton,Bacteria,Biodiversity,lakes,Bacteria: classification,Bacterial Physiological Phenomena,Bacteria: metabolism,Lakes: microbiology,Phytoplankton: microbiology,Water Microbiology},
  author = {Paver, Sara F and Hayek, Kevin R and a Gano, Kelsey and Fagen, Jennie R and Brown, Christopher T and Davis-Richardson, Austin G and Crabb, David B and Rosario-Passapera, Richard and Giongo, Adriana and Triplett, Eric W and Kent, Angela D},
