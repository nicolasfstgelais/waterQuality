@article{lawtonAreThereGeneral1999,
  title = {Are {{There General Laws}} in {{Ecology}}?},
  volume = {84},
  issn = {00301299},
  url = {http://www.jstor.org/stable/3546712 http://www.jstor.org/stable/10.2307/3546712},
  abstract = {The dictionary definition of a law is: "Generalized formulation based on a series of events or processes observed to recur regularly under certain conditions; a widely observable tendency". I argue that ecology has numerous laws in this sense of the word, in the form of widespread, repeatable patterns in nature, but hardly any laws that are universally true. Typically, in other words, ecological patterns and the laws, rules and mechanisms that underpin them are contingent on the organisms involved, and their environment. This contingency is manageable at a relatively simple level of ecological organisation (for example the population dynamics of single and small numbers of species), and re-emerges also in a manageable form in large sets of species, over large spatial scales, or over long time periods, in the form of detail-free statistical patterns - recently called 'macroecology'. The contingency becomes overwhelmingly complicated at intermediate scales, characteristic of community ecology, where there are a large number of case histories, and very little other than weak, fuzzy generalisations. These arguments are illustrated by focusing on examples of typical studies in community ecology, and by way of contrast, on the macroecological relationship that emerges between local species richness and the size of the regional pool of species. The emergent pattern illustrated by local vs regional richness plots is extremely simple, despite the vast number of contingent processes and interactions involved in its generation. To discover general patterns, laws and rules in nature, ecology may need to pay less attention to the 'middle ground' of community ecology, relying less on reductionism and experimental manipulation, but increasing research efforts into macroecology. CR - Copyright \&\#169; 1999 Nordic Society Oikos},
  number = {2},
  journaltitle = {Oikos},
  date = {1999-02},
  pages = {177--192},
  author = {Lawton, John},
  file = {C:\\Users\\nicol\\Zotero\\storage\\4QQRH4WH\\Lawton - 1999 - Are There General Laws in Ecology(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\6U47A9HD\\Lawton - 1999 - Are there general laws in ecology.pdf;C:\\Users\\nicol\\Zotero\\storage\\9W35TSJQ\\Lawton - 1999 - Are There General Laws in Ecology(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\CBQNACBU\\Lawton - 1999 - Are There General Laws in Ecology(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\CSH2HQS5\\Lawton - 1999 - Are there general laws in ecology.pdf;C:\\Users\\nicol\\Zotero\\storage\\QZKDKRMA\\Lawton - 1999 - Are there general laws in ecology.pdf;C:\\Users\\nicol\\Zotero\\storage\\RBGNIN2F\\Lawton - 1999 - Are there general laws in ecology.pdf;C:\\Users\\nicol\\Zotero\\storage\\UFV6R3UD\\Lawton - 1999 - Are There General Laws in Ecology(2).pdf;C:\\Users\\nicol\\Zotero\\storage\\X8K639BT\\Lawton - 1999 - Are there general laws in ecology.pdf;C:\\Users\\nicol\\Zotero\\storage\\XBAA482W\\Lawton - 1999 - Are There General Laws in Ecology(2).pdf}
