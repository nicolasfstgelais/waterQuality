@article{watsonPatternsPhytoplanktonTaxonomic1997,
  title = {Patterns in {{Phytoplankton Taxonomic Composition Across Temperate Lakes}} of {{Differing Nutrient Status}}},
  volume = {42},
  abstract = {We describe the relationships between summer average total phosphorus (TP) and the biomass of six major phytoplankton taxonomic groups from 91 north temperate lakes. Both regression and locally weighted smoothing (LOWESS) analyses show that all groups increase with TP but over different nutrient ranges. At nutrient extremes, i.e. very low and high levels of TP, the few taxonomic groups that dominate total biomass are generally dissmilar. In oligotrophic and eutrophic lakes, most group biomass curves show corresponding increases or decreases, but different rates of changes. The curves converge at intermediate TP levels \$($\backslash$backslashsim10-30 $\backslash$backslashmug lite\^-1)\$ where there is increased equitability amongalgal groups. In highly eutrophic lakes, the slope of the total biomass curve decreases, as do all the relative abundances of all groups except blue-greens and diatoms. We conclude that the curvilinearity of the TP-total biomass curve is not attributable to a single taxonomic groups, because all groups show some nonlinearity in relation to TP. We suggest that morphological diversity, differential herbivory, and, in particular, mixing regime may explain some of the observed patterns.},
  number = {3},
  journaltitle = {Limnology and Oceanography},
  date = {1997},
  pages = {487--495},
  author = {Watson, Susan B and McCauley, Edward and Downing, John A},
  file = {C:\\Users\\nicol\\Zotero\\storage\\Z2JCSEIU\\Watson, McCauley, Downing - 1997 - Patterns in Phytoplankton Taxonomic Composition Across Temperate Lakes of Differing Nutrient Status.pdf}
}

