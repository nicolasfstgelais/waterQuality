  file = {C:\\Users\\nicol\\Zotero\\storage\\8RME8KMT\\LEBRIJA-TREJOS et al. - 2010 - Functional traits and environmental filtering drive community assembly in a species-rich tropical system.pdf;C:\\Users\\nicol\\Zotero\\storage\\A2NZZ8ET\\LEBRIJA-TREJOS et al. - 2010 - Functional traits and environmental filtering drive community assembly in a species-rich tropical system.pdf;C:\\Users\\nicol\\Zotero\\storage\\UV589H6Q\\LEBRIJA-TREJOS et al. - 2010 - Functional traits and environmental filtering drive community assembly in a species-rich tropical system.pdf}
