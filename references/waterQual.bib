  url = {http://pubs.acs.org/doi/pdfplus/10.1021/es034905z},
  doi = {10.1021/es034905z},
  abstract = {This paper presents a framework for analyzing the economic, health, and recreation implications of swim closures related to high fecal indicator bacteria (FIB) levels. The framework utilizes benefit transfer policy analysis to provide a practical procedure for estimating the effectiveness of recreational water quality policies. Evaluation criteria include the rates of intended and unintended management outcomes, whether the chosen protocols generate closures with positive net economic benefits to swimmers, and the number of predicted illnesses the policy is able to prevent. We demonstrate the framework through a case study of a Lake Michigan freshwater beach using existing water quality and visitor data from 1998 to 2001. We find that a typical closure causes a net economic loss among would-be swimmers totaling \$1274-37 030/ day, depending on the value assumptions used. Unnecessary closures, caused by high indicator variability and a 24-h time delay between when samples are taken and the management decision can be made, occurred on 14 (12\%) out of 118 monitored summer days. Days with high FIB levels when the swim area is open are also common but do relatively little economic harm in comparison. Also, even if the closure policy could be implemented daily and perfectly without error, only about 42\% of predicted illnesses would be avoided. These conclusions were sensitive to the relative values and risk preferences that swimmers have for recreation access and avoiding health effects, suggesting a need for further study of the impacts of recreational water quality policies on individuals.},
  number = {10},
  journaltitle = {Environmental Science \& Technology},
  date = {2004},
  pages = {2737--2745},
  author = {Rabinovici, Sharyl J. and Bernknopf, Richard L and Wein, Anne M. and Coursey, Don L. and Whitman, Richard L.},
  file = {C:\\Users\\nicol\\Zotero\\storage\\G3MKCP9X\\Rabinovici et al. - 2004 - Economic and Health Risk Trade-Offs of Swim Closures at a Lake Michigan Beach.pdf;C:\\Users\\nicol\\Zotero\\storage\\JSFCME6J\\Wein et al. - 2004 - Economic and health risk trade-offs of swim closures at a Lake Michigan beach.pdf}
}

@article{warwickPracticalMeasuresMarine2001,
  title = {Practical Measures of Marine Biodiversity Based on Relatedness of Species},
  volume = {39},
  issn = {0078-3218},
  abstract = {In contrast to terrestrial biodiversity, marine biodiversity has a number of distinctive\$$\backslash$backslash\$nfeatures that suggest that a broader strategy for its conservation might be more appropriate\$$\backslash$backslash\$nthan a local reserve-based one. Traditional diversity measures based on species richness and\$$\backslash$backslash\$nevenness often have disadvantages in the assessment of biodiversity change on wide spatial\$$\backslash$backslash\$nscales and long timescales. Alternative measures based on the degree of relatedness of species\$$\backslash$backslash\$novercome these problems to varying degrees. They fall into two categories. Taxonomic\$$\backslash$backslash\$ndistinctiveness measures are used as a means of preferentially selecting species from an\$$\backslash$backslash\$ninventory for conservation purposes: species that diverge close to the base of a phylogenetic\$$\backslash$backslash\$nor taxonomic tree and have few close relatives will preserve more evolutionary history than\$$\backslash$backslash\$nthose that diverge further up and have more congeners. Complementary subsets of species\$$\backslash$backslash\$ncan also be selected to provide representativeness of the widest range of evolutionary characters.\$$\backslash$backslash\$nTaxonomic distinctness measures, on the other hand, are a means of comparing patterns of\$$\backslash$backslash\$nrelatedness in community samples in the field, and monitoring changes in these patterns over\$$\backslash$backslash\$nspace or time. They measure either the average distance apart of all pairs of individuals or\$$\backslash$backslash\$nspecies in a sample, traced through a taxonomic tree, or the variability in structure across the\$$\backslash$backslash\$ntree. These measures are beginning to find application in broad scale geographical comparisons\$$\backslash$backslash\$nof biodiversity, in environmental impact assessment and in evaluation of surrogates for\$$\backslash$backslash\$nbiodiversity estimation.},
  issue = {January 2001},
  journaltitle = {Oceanography and Marine Biology: An Annual Review},
