@article{baiserSpeciesRichnessTrophic2011,
  title = {Species Richness and Trophic Diversity Increase Decomposition in a Co-Evolved Food Web.},
  volume = {6},
  issn = {1932-6203},
  url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3108618&tool=pmcentrez&rendertype=abstract},
  doi = {10.1371/journal.pone.0020672},
  abstract = {Ecological communities show great variation in species richness, composition and food web structure across similar and diverse ecosystems. Knowledge of how this biodiversity relates to ecosystem functioning is important for understanding the maintenance of diversity and the potential effects of species losses and gains on ecosystems. While research often focuses on how variation in species richness influences ecosystem processes, assessing species richness in a food web context can provide further insight into the relationship between diversity and ecosystem functioning and elucidate potential mechanisms underpinning this relationship. Here, we assessed how species richness and trophic diversity affect decomposition rates in a complete aquatic food web: the five trophic level web that occurs within water-filled leaves of the northern pitcher plant, Sarracenia purpurea. We identified a trophic cascade in which top-predators–larvae of the pitcher-plant mosquito–indirectly increased bacterial decomposition by preying on bactivorous protozoa. Our data also revealed a facultative relationship in which larvae of the pitcher-plant midge increased bacterial decomposition by shredding detritus. These important interactions occur only in food webs with high trophic diversity, which in turn only occur in food webs with high species richness. We show that species richness and trophic diversity underlie strong linkages between food web structure and dynamics that influence ecosystem functioning. The importance of trophic diversity and species interactions in determining how biodiversity relates to ecosystem functioning suggests that simply focusing on species richness does not give a complete picture as to how ecosystems may change with the loss or gain of species.},
  number = {5},
  journaltitle = {PloS one},
  date = {2011-01},
  pages = {e20672},
  keywords = {Animals,Population Dynamics,Predatory Behavior,Food Chain,Ecosystem,Biodiversity,Biota,Biomass,Culicidae,Culicidae: physiology,Euglenozoa,Euglenozoa: physiology,Sarraceniaceae,Sarraceniaceae: microbiology},
  author = {Baiser, Benjamin and Ardeshiri, Roxanne S and Ellison, Aaron M},
  file = {C:\\Users\\nicol\\Zotero\\storage\\IKTH268Z\\Baiser, Ardeshiri, Ellison - 2011 - Species richness and trophic diversity increase decomposition in a co-evolved food web.pdf},
