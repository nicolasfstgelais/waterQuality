  date = {2010-01-01},
  pages = {119-137},
  author = {Finkel, Zoe V. and Beardall, John and Flynn, Kevin J. and Quigg, Antonietta and Rees, T. Alwyn V. and Raven, John A.},
  file = {C:\\Users\\nicol\\Zotero\\storage\\TMMS5VIC\\Finkel et al. - 2010 - Phytoplankton in a changing world Cell size and elemental stoichiometry.pdf;C:\\Users\\nicol\\Zotero\\storage\\VWVSITVA\\Finkel et al. - 2010 - Phytoplankton in a changing world cell size and e.pdf;C:\\Users\\nicol\\Zotero\\storage\\RHZPWGDI\\1492394.html}
