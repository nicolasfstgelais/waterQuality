  keywords = {zooplankton,Animals,Fresh Water,Temperature,Carbon,Carbon: analysis,Biodiversity,Climate,Climate change,Europe,Principal component analysis,North America,Fresh Water: analysis,Hydrogen-Ion Concentration},
  author = {Shurin, Jonathan B and Winder, Monika and Adrian, Rita and Keller, Wendel Bill and Matthews, Blake and Paterson, Andrew M and Paterson, Michael J and Pinel-Alloul, Bernadette and a Rusak, James and Yan, Norman D},
  file = {C:\\Users\\nicol\\Zotero\\storage\\G9KRNB9V\\Shurin et al. - 2010 - Environmental stability and lake zooplankton diversity - contrasting effects of chemical and thermal variability.pdf},
