  file = {C:\\Users\\nicol\\Zotero\\storage\\8DCR6NGH\\Harwood et al. - 2014 - Microbial source tracking markers for detection of fecal contamination in environmental waters Relationships bet.pdf;C:\\Users\\nicol\\Zotero\\storage\\F8PCEDGP\\Harwood et al. - 2014 - Microbial source tracking markers for detection of fecal contamination in environmental waters Relationships bet.pdf},
